        <Button Content="帮助" HorizontalAlignment="Left"  VerticalAlignment="Top" Width="46" Height="30" Click="Button_Click_help" RenderTransformOrigin="-0.305,0.564" Margin="863,18,0,0"/>
        <Button Content="清空log" HorizontalAlignment="Left" Margin="621,18,0,0" VerticalAlignment="Top" Width="58" Height="30" Click="Button_Click_Clear_MTK_Log"/>
